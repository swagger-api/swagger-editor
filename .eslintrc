{
  "extends": [
    "react-app",
    "react-app/jest",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "plugin:prettier/recommended",
    "plugin:cypress/recommended"
  ],
  "globals": {
    "File": true,
    "Blob": true,
    "cy": false,
    "Cypress": false
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "jsx-a11y",
    "prettier"
  ],
  "rules": {
    "import/order": ["error", {
      "groups": [
        ["builtin", "external", "internal"],
        ["parent", "sibling", "index"]
      ],
      "newlines-between": "always"
    }],
    "prettier/prettier": "error",
    "import/no-unresolved": 0
  }
}
